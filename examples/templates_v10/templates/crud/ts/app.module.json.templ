        "Dialog${ obj.cls }Component",
% endif
% if obj.actions.has( 'new' ) and obj.actions.isScreen( 'new' ) or  obj.actions.has( 'edit' ) and obj.actions.isScreen( 'edit' ):
        "Screen${ obj.cls }Component",
